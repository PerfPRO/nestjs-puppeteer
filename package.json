{
  "name": "nestjs-puppeteer",
  "description": "Puppeteer module for Velocity NestJS",
  "version": "0.2.0",
  "license": "UNLICENSED",
  "author": "Vision Quest Virtual LLC",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "watch": "rm -rf dist && tsc -w -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",    
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "npm": ">=10.8.0",
    "node": ">=20.16.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {    
    "@nestjs/common": "^10.4.1",    
    "@nestjs/core": "^10.4.1",
    "@nestjs/platform-express": "10.4.1",
    "@nestjs/testing": "^10.4.1",
    "@types/node": "^22.5.0",                   
    "puppeteer": "^23.1.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",        
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0 || ^10.0",
    "@nestjs/core": "^9.0.0 || ^10.0",
    "puppeteer": "^23",
    "puppeteer-extra": "^3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oblakstudio/nestjs-puppeteer.git"
  }
}
